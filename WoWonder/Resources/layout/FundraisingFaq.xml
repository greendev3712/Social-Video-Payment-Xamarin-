<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.core.widget.NestedScrollView
            android:id="@+id/nested_scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipToPadding="false"
            android:scrollbars="none"
            android:scrollingCache="true"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="15dp">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:text="@string/Lbl_AmericaFirstFundraising"
					android:textSize="20sp"
                    android:textStyle="bold"
                    android:textAllCaps="true"
					android:textAlignment="center"/>
				<TextView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:text="@string/Lbl_FrequentlyAskedQuestions"
                    android:layout_marginTop="20dp"
                    android:textStyle="bold"
					android:textAlignment="center"
					android:textSize="18sp"/>
				<ImageView
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:src="@drawable/fundraiser"
                    android:scaleType="centerCrop"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_HowDoesFundraisingAFPWorkTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_HowDoesFundraisingAFPWorkContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_HowDoesFundraisingAFPWorkTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_HowDoesFundraisingAFPWorkContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_WhatCanRaiseMoneyTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_WhatCanRaiseMoneyContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_HowDiWithdrawTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_HowDiWithdrawContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_HowDiKnowFundrasingTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_HowDiKnowFundrasingContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_CanAnotherPersonWithdrawTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_CanAnotherPersonWithdrawContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_DeadlineOrTimeLimitTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_DeadlineOrTimeLimitContent"
					android:textSize="12sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="center"
					android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
					android:text="@string/Lbl_DontReachGoalTitle"
                    android:textStyle="bold"
					android:textSize="16sp"/>
				<TextView
					android:layout_width="match_parent"
					android:textAlignment="textStart"
					android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
					android:text="@string/Lbl_DontReachGoalContent"
					android:textSize="12sp"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</LinearLayout>